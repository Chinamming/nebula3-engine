IDR_MAINFRAME MENU 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "New(&N)",                     ID_Menu
        MENUITEM "Open...",                     ID_FILE_OPEN
        MENUITEM "Save",                        ID_FILE_SAVE40002
        MENUITEM "Save As...",                  ID_FILE_SAVEAS
        MENUITEM SEPARATOR
        POPUP "Import"
        BEGIN
            MENUITEM "collada file(*.dae)",         ID_40017
        END
        MENUITEM SEPARATOR
        MENUITEM "Show Log File",               ID_FILE_SHOWLOGFILE
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
    END
    POPUP "Modify(&M)"
    BEGIN
        MENUITEM "Link(&L)",                    ID_MODIFY_LINK
    END
    POPUP "Terrain(&T)"
    BEGIN
        MENUITEM "&Edit Terrain...",            ID_TERRAIN_E
    END
    POPUP "Sound(&S)"
    BEGIN
        MENUITEM "&Open Sound Brower",          ID_SOUND_OPENSOUNDBROWER
    END
    POPUP "Game(&G)"
    BEGIN
        MENUITEM "&Swith to Game",              ID_GAME_SWITHTOGAME
    END
    MENUITEM "AI",                          ID_AI
    POPUP "T&ools"
    BEGIN
        MENUITEM "&Reload Script",              ID_TOOLS_RELOADSCRIPT
    END
    POPUP "&View"
    BEGIN
        POPUP "&Toolbars"
        BEGIN
            MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
            MENUITEM SEPARATOR
            MENUITEM "&Customize...",               35001
        END
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About Nebula3 Editor...",    ID_APP_ABOUT
    END
END


